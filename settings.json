{
    "editor": {
        "copyWithSyntaxHighlighting": false,
        "emptySelectionClipboard": false,
        "multiCursorModifier": "ctrlCmd",
        "snippetSuggestions": "top",
        "detectIndentation": false,
        "lineNumbers": "on",
        "minimap": {
            "enabled": false
        },
        "guides": {
            "indentation": false
        },
        "fontFamily": "Caskaydiacove Nerd Font Mono, 'JetBrains Mono', 'Fira Code', 'Cascadia Code', 'Source Code Pro', 'Menlo', 'Consolas', 'Monaco', 'Courier New', monospace",
        "fontSize": 16,
        "lineHeight": 32,
        "wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
        "hover": {
            "delay": 1500,
            "enabled": true
        },
        "renderLineHighlight": "none",
        "quickSuggestions": {
            "strings": true
        },
        "linkedEditing": true,
        "colorDecorators": true,
        "selectionHighlight": false,
        "occurrencesHighlight": "off",
        "overviewRulerBorder": true,
        "hideCursorInOverviewRuler": true,
        "cursorBlinking": "solid",
        "cursorSmoothCaretAnimation": "on",
        "formatOnPaste": false,
        "tabSize": 4,
        "autoIndent": "full",
        "scrollbar": {
            "verticalScrollbarSize": 5,
            "horizontalScrollbarSize": 5
        },
        "tokenColorCustomizations": {
            "textMateRules": [
                {
                    "scope": [
                        "comment",
                        "keyword",
                        "variable.language",
                        "entity.other.attribute-name.html",
                        "entity.other.attribute-name",
                        "keyword.control",
                        "storage.type",
                        "comment.block",
                        "comment.line"
                    ]
                }
            ]
        }
    },
    "window": {
        "newWindowDimensions": "inherit",
        "nativeFullScreen": true,
        "titleBarStyle": "custom",
        "closeWhenEmpty": false,
        "restoreWindows": "all"
    },
    "files": {
        "trimTrailingWhitespace": true,
        "insertFinalNewline": true,
        "trimFinalNewlines": true,
        "autoSave": "afterDelay",
        "associations": {
            ".php_cs": "php",
            ".php_cs.dist": "php"
        },
        "exclude": {
            "**/tmp/**": true,
            "**/node_modules/**": true,
            "**/vendor/{[^l],?[^ai]}*": true,
            "**/public/{[^i],?[^n]}*": true,
            "**/dist": true,
            "**/_ide_helper.php": true,
            "**/composer.lock": true,
            "**/package-lock.json": true,
            "storage": true,
            ".phpunit.result.cache": true
        }
    },
    "search": {
        "useIgnoreFiles": false,
        "exclude": {
            "**/*.bundle.js": true,
            "**/*all.js": true,
            "**/Library": true,
            "~/Library**": true
        }
    },
    "security.workspace.trust.untrustedFiles": "open",
    "notebook.output.wordWrap": true,
    "emmet.includeLanguages": {
        "blade": "html",
        "vue-html": "html",
        "vue": "html",
        "react": "html",
        "javascript": "html"
    },
    "workbench.tree.enableStickyScroll": false,
    "workbench.tree.indent": 15,
    "workbench.editor.limit.value": 20,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorTheme": "Default High Contrast",
    "workbench.iconTheme": "vs-seti",
    "window.zoomLevel": 0,
    "editor.fontLigatures": true,
    "editor.rulers": [
        80,
        120
    ],
    "editor.formatOnSave": true,
    "editor.formatOnSaveTimeout": 750,
    "editor.formatOnPaste": true,
    "files.encoding": "utf8",
    "files.autoGuessEncoding": true,
    "files.autoSaveDelay": 1000,
    "explorer.compactFolders": false,
    "explorer.decorations.badges": true,
    "breadcrumbs.enabled": true,
    "editor.semanticHighlighting.enabled": true,
    "editor.semanticTokenColorCustomizations": {
        "enabled": true,
        "rules": {
            "variable": {
                "foreground": "#9CDCFE"
            },
            "variable.readonly": {
                "foreground": "#4EC9B0",
                "fontStyle": "italic"
            },
            "parameter": {
                "foreground": "#C586C0"
            },
            "property": {
                "foreground": "#9CDCFE"
            },
            "function": {
                "foreground": "#DCDCAA"
            },
            "method": {
                "foreground": "#DCDCAA"
            },
            "keyword": {
                "foreground": "#569CD6",
                "fontStyle": "bold"
            },
            "class": {
                "foreground": "#4EC9B0",
                "fontStyle": "bold"
            },
            "interface": {
                "foreground": "#B8D7A3"
            },
            "enum": {
                "foreground": "#B8D7A3"
            },
            "type": {
                "foreground": "#4EC9B0"
            },
            "string": {
                "foreground": "#CE9178"
            },
            "number": {
                "foreground": "#B5CEA8"
            },
            "comment": {
                "foreground": "#6A9955",
                "fontStyle": "italic"
            },
            "regexp": {
                "foreground": "#D16969"
            }
        }
    },
    "workbench.colorCustomizations": {
        "editorError.foreground": "#F44747",
        "editorWarning.foreground": "#FF8800",
        "editorInfo.foreground": "#9CDCFE",
        "editorHint.foreground": "#D7BA7D",
        "editorError.border": "#F44747",
        "editorWarning.border": "#FF8C00",
        "editorGutter.modifiedBackground": "#D7BA7D",
        "editorGutter.addedBackground": "#4EC9B0",
        "editorUnnecessary.foreground": "#7F848E"
    },
    "problems.decorations.enabled": true,
    "editor.renderWhitespace": "none",
    "editor.renderControlCharacters": true,
    "workbench.list.smoothScrolling": true,
    "editor.smoothScrolling": true,
    "terminal.integrated.smoothScrolling": true,
    "editor.codeLensFontFamily": "Caskaydiacove Nerd Font Mono, 'JetBrains Mono', 'Fira Code', 'Cascadia Code', 'Source Code Pro', 'Consolas', monospace",
    "editor.inlayHints.fontFamily": "Caskaydiacove Nerd Font Mono, 'JetBrains Mono', 'Fira Code', 'Cascadia Code', 'Source Code Pro', 'Consolas', monospace",
    "editor.inlineSuggest.fontFamily": "Caskaydiacove Nerd Font Mono, 'JetBrains Mono', 'Fira Code', 'Cascadia Code', 'Source Code Pro', 'Consolas', monospace",
    "debug.console.fontFamily": "Caskaydiacove Nerd Font Mono, 'Cascadia Code', 'Consolas', 'Courier New', monospace",
    "scm.inputFontFamily": "Caskaydiacove Nerd Font Mono, 'JetBrains Mono', 'Fira Code', 'Cascadia Code', monospace",
    "terminal.integrated.fontFamily": "Caskaydiacove Nerd Font Mono, 'Cascadia Code', 'Consolas', 'Fira Code', 'JetBrains Mono', 'Source Code Pro', monospace",
    "notebook.output.fontFamily": "Caskaydiacove Nerd Font Mono, 'JetBrains Mono', 'Fira Code', monospace",
    "chat.editor.fontFamily": "Caskaydiacove Nerd Font Mono, 'Segoe UI', 'Inter', system-ui",
    "markdown.preview.fontFamily": "Caskaydiacove Nerd Font Mono, 'Segoe UI', 'Inter', system-ui",
    "window.commandCenter": false,
    "editor.cursorStyle": "line-thin",
    "editor.cursorBlinking": "smooth",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorStyleInactive": "line",
    "notebook.multiCursor.enabled": true,
    "editor.scrollbar.vertical": "hidden",
    "editor.hideCursorInOverviewRuler": true,
    "workbench.editor.labelFormat": "short",
    "workbench.editor.empty.hint": "hidden",
    "workbench.sideBar.location": "right",
    "window.menuBarVisibility": "toggle",
    "workbench.activityBar.location": "bottom",
    "workbench.layoutControl.enabled": false,
    "terminal.integrated.hideOnStartup": "always",
    "workbench.editor.showTabs": "multiple",
    "editor.guides.bracketPairs": true,
    "editor.guides.bracketPairsHorizontal": true,
    "window.customTitleBarVisibility": "auto",
    "codeium.enableCodeLens": false,
    "redhat.telemetry.enabled": true
}
